  "scripts": {
    "prepare:browser": "node scripts/parse_config && cordova prepare browser",
    "prepare:android": "node scripts/parse_config && cordova prepare android",
    "release:android": "node scripts/parse_config && cordova build --release android",
    "prepare:ios": "node scripts/parse_config && cordova prepare ios",
    "compile": "cross-env UV_THREADPOOL_SIZE=100 webpack",
    "start": "node scripts/parse_config && webpack-dev-server --inline --hot --content-base ./platforms/browser/www",
    "run:android": "export PLATFORM=ANDROID && node scripts/parse_config && cordova run android --device",
    "run:ios": "export PLATFORM=IOS && node scripts/parse_config && cordova run ios --device",
    "emulate:android": "export PLATFORM=ANDROID && node scripts/parse_config && cordova emulate android",
    "emulate:ios": "export PLATFORM=IOS && node scripts/parse_config && cordova emulate ios --device",
    "build:ios": "export PLATFORM=IOS && node scripts/parse_config && cordova build ios",
    "build:android": "export PLATFORM=ANDROID && node scripts/parse_config && cordova build android",
    "release": "node scripts/parse_config && cross-env NODE_ENV=production TARGET=release cordova build --release",
    "resources": "splashicon-generator --imagespath=\"res\"",
    "drawables": "ncp res/android-drawable platforms/android/res",
    "config": "node scripts/parse_config"
  },
  "license": "ISC",
